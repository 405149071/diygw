{
    "name": "diygw/diygw-addons",
    "description": "addons package for diygw",
    "homepage": "https://github.com/diygw/diygw-addons",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "version": "1.1.2",
    "authors": [
        {
            "name": "diygwcom",
            "email": "diygwcom@foxmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/diygw/diygw-addons/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "topthink/think-helper": ">=1.0.4",
        "topthink/think-installer": ">=1.0.10"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src/"
        },
        "files": [
            "src/common.php"
        ]
